{
   "compilerOptions": {
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
         "~/*": ["./dist/src/*"],
         "@prisma-gen/*": ["./dist/prisma/generated/*"],
         "@lib/*": ["./dist/src/lib/*"],
         "@utils/*": ["./dist/src/utils/*"],
         "@c-types/*": ["./dist/src/types/*"],
         "@resolvers/*": ["./dist/src/resolvers/*"],
         "@handlers/*": ["./dist/src/handlers/*"],
         "@middlewares/*": ["./dist/src/middlewares/*"],
         "@config/*": ["./dist/src/config/*"],
         "@auth/*": ["./dist/src/auth/*"]
      }
   }
}
