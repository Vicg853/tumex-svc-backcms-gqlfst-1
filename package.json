  "description": "",
  "main": "dist/index",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      ".yarn",
      ".github",
      "dist"
    ],
    "watch": [
      "src",
      "prisma"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "test": "NODE_ENV=test && echo 'test'",
    "generate": "npx prisma generate",
    "dev:gen": "npx prisma generate --watch",
    "dev": "nodemon",
    "build": "tsc",
    "build:paths": "tscpaths -p tsconfig-build-paths.json -s ./dist -o ./dist",
    "build:all": "yarn generate && yarn build && yarn build:paths",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vicg853/tumex-svc-backcms-gqlfst-1.git"
  },
  "keywords": [
    "api",
    "grapqhl",
    "rest",
    "cms",
    "prisma",
    "postgresql"
  ],
